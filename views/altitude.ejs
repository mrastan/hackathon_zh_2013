<div class="qs-box" id="qs-altitude"> 
  <h4 class="qs-box-title">ALTITUDE</h4>    
  <div class="qs-box-content qs-meter">
    <input type="text" class="dial" 
    data-min="0" data-max="500"
    data-width="160" data-height="160"
    data-linecap=round data-angleArc=250 data-angleOffset=-125
    data-thickness=".2"
    data-cursor=false>
    <span class="meter-unit"></span>  
    <span class="meter-icon"><i class="glyphicon-up_arrow"></i></span>    
    <span class="meter-value">305 m</span>
  </div>
</div>  

<script type="text/javascript">
$( function(){
  animateKnob('#qs-altitude', 0, 305, "#ef6510");
});
</script>