<div class="qs-box" id="qs-speed"> 
  <h4 class="qs-box-title">SPEED</h4>          
  <div class="qs-box-content qs-meter">
    <input type="text" class="dial" 
    data-min="0" data-max="50"
    data-width="160" data-height="160"
    data-linecap=round data-angleArc=250 data-angleOffset=-125
    data-thickness=".2"
    data-cursor=false>
    <span class="meter-unit"></span>  
    <span class="meter-icon"><i class="glyphicon-dashboard"></i></span>    
    <span class="meter-value">20.1 km/h</span>
  </div>
</div>  

<script type="text/javascript">
$( function(){
  animateKnob('#qs-speed', 0, 0, "#ef6510");
});
</script>